{
  "name": "skyjo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start-api-prod": "pnpm run --filter api prod",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "format:all": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "test:ui": "turbo run test:ui"
  },
  "devDependencies": {
    "@pnpm/logger": "^5.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "3.2.4",
    "prettier-plugin-organize-imports": "^3.2.3",
    "syncpack": "^12.3.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "packageManager": "pnpm@9.0.6"
}
